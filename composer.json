{
    "name": "lumturo-net/contao-gastroburner-bundle",
    "type": "contao-bundle",
    "description": "Gastroburner-Abfrage-Game",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Rolf Staege",
            "homepage": "http://lumturo.net"
        }
    ],
    "require": {
        "php": "^7.0",
        "contao/core-bundle": "^4.9"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "Lumturo\\ContaoGastroburnerBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Lumturo\\ContaoGastroburnerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lumturo\\ContaoGastroburnerBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/roelfsche/contao-gastroburner-bundle/issues",
        "source": "https://github.com/roelfsche/contao-gastroburner-bundle"
    }
}
